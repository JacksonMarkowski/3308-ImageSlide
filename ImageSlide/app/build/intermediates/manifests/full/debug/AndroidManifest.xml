<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="team16_3308.imageslide"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="team16_3308.imageslide.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="team16_3308.imageslide.DisplayMessageActivity"
            android:parentActivityName="team16_3308.imageslide.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="team16_3308.imageslide.MainActivity" />
        </activity>
        <activity android:name="team16_3308.imageslide.ImageSitesActivity" >
        </activity>
        <activity android:name="team16_3308.imageslide.SettingsActivity" >
        </activity>
    </application>

</manifest>